<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="res_config_settings_view_form" model="ir.ui.view">
            <field name="name">res.config.settings.view.form.inherit.stock.account</field>
            <field name="model">res.config.settings</field>
            <field name="inherit_id" ref="stock.res_config_settings_view_form"/>
            <field name="arch" type="xml">
                <div id="warning_info" position="after">
                        <div class="col-12 col-lg-6 o_setting_box" title="This Feature Allows The Receipt Of Purchase  In Multiple Locations Through Using Multiple Operation Type.">
                                <div class="o_setting_left_pane">
                                    <field name="default_multi_deliver"/>
                                </div>
                                <div class="o_setting_right_pane">
                                    <label for="default_multi_deliver"/>
                                    <div class="text-muted">
                                        This Feature Allows The Receipt Of Purchase  In Multiple Locations Through Using Multiple Operation Type.
                                    </div>
                                </div>
                        </div>
                </div>
            </field>
        </record>
    </data>
</odoo>
