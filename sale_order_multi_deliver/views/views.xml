<odoo>
  <data>
          <record id="view_order_form_inherit_sale_stock" model="ir.ui.view">
            <field name="name">view.order.form.inherit.sale.stock</field>
            <field name="model">sale.order</field>
            <field name="inherit_id" ref="sale_stock.view_order_form_inherit_sale_stock"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='order_line']/form/group/group/field[@name='route_id']" position="before">
                        <field name="picking_type_id" domain="[('code', '=', 'outgoing'),('warehouse_id.company_id','=',parent.company_id)]"/>
                </xpath>        

            </field>
          </record>  
  </data>
</odoo>